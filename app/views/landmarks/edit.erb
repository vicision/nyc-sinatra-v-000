<h1>Update a Landmark</h1>
<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <label>Update Name:</label>
  <input id="hidden" type="hidden" name="_method" value="patch">
  <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>">
  <br></br>
  <label>Enter Year Landmark was Completed</label>
  <br></br>
  <input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed.to_s %>"></input>
  <br></br>
  <input type="submit" name="submit" value="Edit Landmark">
</form>
